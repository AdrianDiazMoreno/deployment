# vim: set ft=sh sw=2 ts=8 et :
deploy_phedex_web_deps()
{
  deploy backend
}

deploy_phedex_web_prep()
{
  mkproj sessions
}

deploy_phedex_web_sw()
{
  deploy_pkg comp cms+PHEDEX-web WEB_3_1_7post1
}

deploy_phedex_web_post()
{
  case $host in vocms10[67] | vocms53 ) disable ;; * ) enable ;; esac
  (mkcrontab; sysboot) | crontab -
}
